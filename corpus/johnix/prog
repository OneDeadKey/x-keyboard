% perl -le 'print "My regex: ", qr/^watch/i'
   My regex: (?i-xsm:^watch this)
   
$rob = qr/red/i;
if ($rob->match("Fred Flintstone")) {
    print "Got obj fred!\n";
}

sub Regexp::match {
    my $self = shift;
    my $arg = @_ ? shift : $_;
    return $arg =~ /$arg/;
}

