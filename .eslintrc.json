{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 2015,
    "sourceType": "module"
  },
  "rules": {
    "no-multi-spaces": "off",
    "array-bracket-spacing": "off",
    "key-spacing": "off",
    "guard-for-in": "off",
    "no-param-reassign": "off",
    "no-underscore-dangle": "off",
    "no-restricted-syntax": "off"
  }
}
